{
	"name": "urn-bld",
	"version": "0.0.1",
	"description": "Uranio builder",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"scripts": {
		"build": "tsc -b",
		"core": "./node_modules/.bin/tsc-watch --onSuccess \"node -r source-map-support/register -r module-alias/register ./dist/.urn/urn-core/run.js\"",
		"web": "./node_modules/.bin/tsc-watch --onSuccess \"node -r source-map-support/register -r module-alias/register ./dist/.urn/urn-web/run.js\"",
		"trx": "./node_modules/.bin/tsc-watch --onSuccess \"node -r source-map-support/register -r module-alias/register ./dist/.urn/urn-trx/run.js\""
	},
	"repository": {
		"type": "git",
		"url": "git+ssh://git@bitbucket.org/nbl7/urn-bld.git"
	},
	"keywords": [
		"uranio",
		"urn"
	],
	"author": "Andrea Reni <mail@nbl7.net>",
	"license": "ISC",
	"homepage": "https://bitbucket.org/nbl7/urn-bld#readme",
	"devDependencies": {
		"urn-web-dep-dev": "git+ssh://git@bitbucket.org/nbl7/urn-web-dep-dev.git",
		"urn-trx-dep-dev": "git+ssh://git@bitbucket.org/nbl7/urn-trx-dep-dev.git"
	},
	"dependencies": {
		"urn-web-dep": "git+ssh://git@bitbucket.org/nbl7/urn-web-dep.git",
		"urn-trx-dep": "git+ssh://git@bitbucket.org/nbl7/urn-trx-dep.git"
	},
	"_moduleAliases": {
		"urn_book": "dist/.urn/book.js",
		"urn_core": "dist/.urn/urn-core/",
		"urn_web": "dist/.urn/urn-web/"
	}
}
