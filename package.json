{
	"name": "urn-bld",
	"version": "0.0.1",
	"description": "Uranio builder",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"scripts": {
		"build": "tsc -b",
		"start": "node -r source-map-support/register -r module-alias/register dist/index.js",
		"dev": "./node_modules/.bin/tsc-watch --onSuccess \"node -r source-map-support/register -r module-alias/register ./dist/index.js\""
	},
	"repository": {
		"type": "git",
		"url": "git+ssh://git@bitbucket.org/nbl7/urn-bld.git"
	},
	"keywords": [
		"uranio",
		"urn"
	],
	"author": "Andrea Reni <mail@nbl7.net>",
	"license": "ISC",
	"homepage": "https://bitbucket.org/nbl7/urn-bld#readme",
	"devDependencies": {},
	"dependencies": {
		"@types/bcrypt": "^3.0.0",
		"@types/jsonwebtoken": "^8.5.0",
		"bcrypt": "^5.0.0",
		"jsonwebtoken": "^8.5.1",
		"module-alias": "^2.2.2",
		"mongoose": "^5.11.11",
		"urn-dev": "git+ssh://git@bitbucket.org/nbl7/urn-dev.git",
		"urn-lib": "git+ssh://git@bitbucket.org/nbl7/urn-lib.git"
	},
	"_moduleAliases": {
		"@book": "dist/book.js"
	}
}
