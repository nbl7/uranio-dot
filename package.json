{
	"name": "urn-dot",
	"version": "0.0.1",
	"description": "Uranio dot",
	"main": "dist/server/index.js",
	"types": "dist/server/index.d.ts",
	"scripts": {
		"server:build": "cd .uranio/server && npx tsc -b",
		"server:dev": "cd .uranio/server && npx tsc -w -p ./tsconfig.json",
		"client:build": "cd .uranio/client && npx nuxt build -c ./nuxt.config.js",
		"client:dev": "cd .uranio/client && npx nuxt dev -c ./nuxt.config.js",
		"client:generate": "cd .uranio/client && npx nuxt generate -c ./nuxt.config.js"
	},
	"repository": {
		"type": "git",
		"url": "git+ssh://git@bitbucket.org/nbl7/urn-dot.git"
	},
	"keywords": [
		"uranio",
		"urn"
	],
	"author": "Andrea Reni <mail@nbl7.net>",
	"license": "ISC",
	"homepage": "https://bitbucket.org/nbl7/urn-dot#readme",
	"devDependencies": {
		"@types/minimist": "^1.2.1",
		"minimist": "^1.2.5",
		"urn-trx-dep-dev": "git+ssh://git@bitbucket.org/nbl7/urn-trx-dep-dev.git"
	},
	"dependencies": {
		"mongoose": "^5.11.15",
		"urn-lib": "git+ssh://git@bitbucket.org/nbl7/urn-lib.git",
		"urn-trx-dep": "git+ssh://git@bitbucket.org/nbl7/urn-trx-dep.git"
	},
	"_moduleAliases": {
		"uranio": "./dist/server/uranio/",
		"uranio-core": "./dist/server/uranio/api/core/",
		"uranio-api": "./dist/server/uranio/api/",
		"uranio-books": "./dist/server/books/"
	}
}
