{
	"name": "urn-dot",
	"version": "0.0.1",
	"description": "Uranio dot",
	"main": "dist/src/index.js",
	"types": "dist/src/index.d.ts",
	"scripts": {
		"build": "tsc -b",
		"dev": "./node_modules/.bin/tsc-watch --onSuccess \"node -r source-map-support/register -r module-alias/register ./dist/src/index.js\"",
		"core": "./node_modules/.bin/tsc-watch --onSuccess \"node -r source-map-support/register -r module-alias/register ./dist/.urn/web/core/run.js\"",
		"web": "./node_modules/.bin/tsc-watch --onSuccess \"node -r source-map-support/register -r module-alias/register ./dist/.urn/web/run.js\"",
		"fnc": "./node_modules/.bin/tsc-watch --onSuccess \"node -r source-map-support/register -r module-alias/register ./dist/.urn/fnc/run.js\"",
		"trx": "./node_modules/.bin/tsc-watch --onSuccess \"node -r source-map-support/register -r module-alias/register ./dist/.urn/trx/run.js\""
	},
	"repository": {
		"type": "git",
		"url": "git+ssh://git@bitbucket.org/nbl7/urn-dot.git"
	},
	"keywords": [
		"uranio",
		"urn"
	],
	"author": "Andrea Reni <mail@nbl7.net>",
	"license": "ISC",
	"homepage": "https://bitbucket.org/nbl7/urn-dot#readme",
	"devDependencies": {
		"urn-fnc-dep": "bitbucket:nbl7/urn-fnc-dep-dev",
		"urn-trx-dep-dev": "git+ssh://git@bitbucket.org/nbl7/urn-trx-dep-dev.git",
		"urn-web-dep-dev": "git+ssh://git@bitbucket.org/nbl7/urn-web-dep-dev.git"
	},
	"dependencies": {
		"mongoose": "^5.11.15",
		"urn-core-dep": "bitbucket:nbl7/urn-core-dep",
		"urn-trx-dep": "git+ssh://git@bitbucket.org/nbl7/urn-trx-dep.git",
		"urn-web-dep": "git+ssh://git@bitbucket.org/nbl7/urn-web-dep.git"
	},
	"_moduleAliases": {
		"urn_books": "./dist/.urn/books.js",
		"uranio": "./dist/.urn/web/"
	}
}
