{
	"name": "urn-dot",
	"version": "0.0.1",
	"description": "Uranio dot",
	"main": "dist/src/index.js",
	"types": "dist/src/index.d.ts",
	"scripts": {
		"build": "tsc -b",
		"dev": "./node_modules/.bin/tsc-watch --onSuccess \"node -r source-map-support/register -r module-alias/register ./dist/src/index.js\"",
		"run": "./node_modules/.bin/tsc-watch --onSuccess \"node -r source-map-support/register -r module-alias/register ./dist/.uranio/lib/run.js\""
	},
	"repository": {
		"type": "git",
		"url": "git+ssh://git@bitbucket.org/nbl7/urn-dot.git"
	},
	"keywords": [
		"uranio",
		"urn"
	],
	"author": "Andrea Reni <mail@nbl7.net>",
	"license": "ISC",
	"homepage": "https://bitbucket.org/nbl7/urn-dot#readme",
	"devDependencies": {
		"@types/minimist": "^1.2.1",
		"minimist": "^1.2.5",
		"urn-core-dep-dev": "bitbucket:nbl7/urn-core-dep-dev",
		"urn-trx-dep-dev": "git+ssh://git@bitbucket.org/nbl7/urn-trx-dep-dev.git",
		"urn-web-dep-dev": "git+ssh://git@bitbucket.org/nbl7/urn-web-dep-dev.git"
	},
	"dependencies": {
		"mongoose": "^5.11.15",
		"urn-core-dep": "bitbucket:nbl7/urn-core-dep",
		"urn-lib": "git+ssh://git@bitbucket.org/nbl7/urn-lib.git",
		"urn-trx-dep": "git+ssh://git@bitbucket.org/nbl7/urn-trx-dep.git",
		"urn-web-dep": "git+ssh://git@bitbucket.org/nbl7/urn-web-dep.git"
	},
	"_moduleAliases": {
		"uranio-books": "./dist/.uranio/server/books/",
		"uranio-books/*": "./dist/.uranio/server/books/*",
		"uranio-books-client": "./dist/.uranio/server/books/",
		"uranio-books-client/*": "./dist/.uranio/server/books/*",
		"uranio": "./dist/.uranio/lib/",
		"uranio-client": "./dist/.uranio/lib/client",
		"uranio-core": "./dist/.uranio/lib/api/core/",
		"uranio-api": "./dist/.uranio/lib/api/"
	}
}
